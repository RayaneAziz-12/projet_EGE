{% set selectedAnnee = app.session.get('annee_scolaire') %}
<div class="section-body" id="page_top">
    <div class="container-fluid">
        <div class="page-header">
            <div class="left">
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="Mots clés">
                    <div class="input-group-append">
                        <button class="btn btn-outline-secondary" type="button">Chercher</button>
                    </div>
                </div>
            </div>
            <div class="right">
                {% if selectedAnnee is not null %}
                    <span class="tag tag-success">{{ selectedAnnee.dateDebut|date('M Y') }} - {{ selectedAnnee.dateFin|date('M Y') }} </span>
                {% endif %}
                <ul class="nav nav-pills">
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Annee scolaire</a>
                        <div class="dropdown-menu">
                            {% for annee in annees %}
                                {% if annee.statut.id == 3 %}
                                    <a class="dropdown-item annee_scolaire" data-id="{{ annee.id }}" href="#">{{ annee.dateDebut|date('M Y') }} - {{ annee.dateFin|date('M Y') }} <small>({{ annee.statut.statutName }})</small></a>
                                {% elseif annee.statut.id == 1 %}
                                    <a class="dropdown-item annee_scolaire" data-id="{{ annee.id }}" href="#">{{ annee.dateDebut|date('M Y') }} - {{ annee.dateFin|date('M Y') }} <small>({{ annee.statut.statutName }})</small></a>
                                {% elseif annee.statut.id == 2 %}
                                    <a class="dropdown-item annee_scolaire" data-id="{{ annee.id }}" href="#">{{ annee.dateDebut|date('M Y') }} - {{ annee.dateFin|date('M Y') }} <small>({{ annee.statut.statutName }})</small></a>
                                {% elseif annee.statut.id == 4 %}
                                    <a class="dropdown-item annee_scolaire" data-id="{{ annee.id }}" href="#">{{ annee.dateDebut|date('M Y') }} - {{ annee.dateFin|date('M Y') }} <small>({{ annee.statut.statutName }})</small></a>
                                {% endif %}
                            {% endfor %}
